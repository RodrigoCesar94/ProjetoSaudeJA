package br.com.generation.ProjetoSaudeJA;

public interface InterfacePaciente {

	int pergunta1(); // Sintomas de alteração dos sentidos.
	int pergunta2(); // dor de cabeça
	int pergunta3(); // febre
	int pergunta4(); // tosse - sim ou não
	int pergunta5(); // Falta de ar 
	int pergunta8(); // tosse seca ou carregada
	String pergunta6(); //  nome do paciente
	int pergunta7(); // idade


}
